{
  "compilerOptions": {
    "allowJs": true,
    "declaration": true,
    "declarationMap": false,
    "emitDeclarationOnly": false,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "lib": ["DOM", "DOM.Iterable", "ScriptHost", "ES2021"],
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "target": "ES2021",
    "types": ["node", "webpack-env"],
    "useDefineForClassFields": true
  }
}
