{
  "name": "@paulpogo/plugin-iac-support-common",
  "description": "Common functionalities for the iac-support plugin",
  "version": "0.1.6",
  "license": "MIT",
  "main": "index.ts",
  "types": "index.ts",
  "publishConfig": {
    "access": "public",
    "main": "dist/index.cjs.js",
    "module": "dist/index.esm.js",
    "types": "dist/index.d.ts"
  },
  "exports": {
    ".": "./index.ts",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "package.json": [
