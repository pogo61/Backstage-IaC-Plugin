{
  "name": "@paulpogo/plugin-iac-support-common",
  "description": "Common functionalities for the iac-support plugin - adds support for the spec.parent attribute on the ResourceComponent entity, so that nested ResourceComponents are shown like that in the Catalog Graph.",
  "engines": {
    "node": "18 || 20"
  },
  "version": "0.1.61",
  "license": "MIT",
  "packageManager": "yarn@1.22.22",
  "main": "./dist/index.js",
  "types": "index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": "./dist/index.js"
  },
  "typesVersions": {
    "*": {
      "package.json": ["package.json"]
    }
  },
  "backstage": {
    "role": "common-library"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsc"
  },
  "dependencies": {
    "@backstage/backend-plugin-api": "^0.6.9",
    "@backstage/catalog-model": "^1.4.3",
    "@backstage/plugin-catalog-node": "^1.6.1",
    "@backstage/plugin-permission-common": "^0.7.12",
    "@backstage/types": "^1.1.1"
  },
  "files": [
    "dist"
  ]
}
