{
  "extends": "../../tsconfig.json",
  "include": ["src"],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["dist/*"]
    },
    "target": "ES2019",
     "outDir": "dist",
     "rootDir": "src",
     "allowJs": true,
     "jsx": "react-jsx",
     "useUnknownInCatchVariables": false,
     "esModuleInterop": true,
     "types": [
       "node"
     ]
  }
}
