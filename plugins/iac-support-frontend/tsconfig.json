{
  "extends": "../../tsconfig.json",
  "include": [
    "./*"
  ],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "target": "ES2019",
     "outDir": "dist",
     "rootDir": ".",
     "allowJs": true,
     "jsx": "react-jsx",
     "skipLibCheck": true,
     "useUnknownInCatchVariables": false,
     "esModuleInterop": true,
     "types": [
       "node",
       "@types/webpack-env"
     ]
  }
}
